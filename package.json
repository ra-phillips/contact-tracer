    "type": "git",
    "url": "git+https://github.com/ra-phillips/contact-tracer.git"
  },
  "author": "Ranaco Phillips",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ra-phillips/contact-tracer/issues"
  },
  "homepage": "https://github.com/ra-phillips/contact-tracer#readme",
  "dependencies": {
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.13"
  },
  "devDependencies": {}
}
